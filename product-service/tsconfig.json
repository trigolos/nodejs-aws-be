{
    "compilerOptions": {
        "lib": ["es2020"],
        "removeComments": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "target": "es2019",
        "outDir": "lib"
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules/**/*", ".serverless/**/*", ".webpack/**/*", "_warmup/**/*", ".vscode/**/*"]
}
